{
  "name": "react-training-koombea",
  "version": "1.0.0",
  "description": "React training basics",
  "main": "index.js",
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "browserify scripts/index.js -o build/index.js",
