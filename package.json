    "serve": "budo scripts/index.js:build/index.js -v --live -- -p errorify | garnish -v"
  },
  "keywords": [
    "React",
    "koombea"
  ],
  "author": "Richard Roncancio",
  "license": "ISC",
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "budo": "^7.1.0",
    "errorify": "^0.3.1",
    "garnish": "^5.0.1",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "react": "^0.14.6",
    "react-dom": "^0.14.6",
    "sort-by": "^1.1.1"
